{
  "manifest_version": 2,
  "name": "Sapiens Reloader",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "*://sapiens.dti.ufv.br/sapiens/Utilitarios/*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "webNavigation"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
